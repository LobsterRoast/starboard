<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.3 -->
<interface>
  <!-- interface-name starboard.ui -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkApplicationWindow" id="window">
    <property name="default-height">480</property>
    <property name="default-width">640</property>
    <child>
      <object class="GtkBox" id="content">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox" id="toolbar">
            <property name="name">toolbar</property>
            <child>
              <object class="GtkButton" id="github-button">
                <property name="icon-name">github-logo</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="help-button">
                <property name="icon-name">help-icon</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="dashboard">
            <property name="margin-bottom">20</property>
            <property name="margin-end">20</property>
            <property name="margin-start">20</property>
            <property name="margin-top">20</property>
            <property name="vexpand">True</property>
            <child>
              <object class="GtkBox" id="options">
                <property name="margin-end">20</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkBox">
                    <property name="halign">start</property>
                    <property name="spacing">10</property>
                    <property name="valign">center</property>
                    <child>
                      <object class="GtkSwitch" id="toggle-switch">
                        <property name="action-name">actions.activate-starboard</property>
                        <property name="halign">center</property>
                        <property name="height-request">60</property>
                        <property name="valign">start</property>
                        <property name="width-request">120</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCenterBox">
                        <child type="center">
                          <object class="GtkLabel" id="status-label">
                            <property name="halign">center</property>
                            <property name="label">Status:</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <property name="vexpand">True</property>
                    <child>
                      <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="valign">center</property>
                        <child>
                          <object class="GtkEntry" id="ip-box">
                            <property name="margin-bottom">20</property>
                            <property name="max-length">15</property>
                            <property name="placeholder-text">IP (i.e. 255.255.255.255)</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkEntry" id="port-box">
                            <property name="margin-bottom">20</property>
                            <property name="max-length">5</property>
                            <property name="placeholder-text">Port (i.e. 8080)</property>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkBox" id="text-info">
                    <property name="css-classes">
</property>
                    <property name="margin-bottom">20</property>
                    <property name="orientation">vertical</property>
                    <property name="valign">start</property>
                    <child>
                      <object class="GtkLabel" id="device-status">
                        <property name="halign">start</property>
                        <property name="label">Device: Connected</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="latency">
                        <property name="halign">start</property>
                        <property name="label">Latency: 0 ms</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="battery">
                        <property name="halign">start</property>
                        <property name="label">Battery: 100% (Discharging)</property>
                      </object>
                    </child>
                    <style>
                      <class name="info-box"/>
                    </style>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkSeparator"/>
            </child>
            <child>
              <object class="GtkBox" id="device-info">
                <property name="hexpand">True</property>
                <property name="margin-start">20</property>
                <property name="name">object-info</property>
                <property name="orientation">vertical</property>
                <property name="width-request">500</property>
                <child>
                  <object class="GtkBox">
                    <property name="hexpand">True</property>
                    <property name="homogeneous">True</property>
                    <property name="vexpand">True</property>
                    <child>
                      <object class="GtkBox">
                        <property name="hexpand">True</property>
                        <property name="orientation">vertical</property>
                        <property name="vexpand">True</property>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Buttons</property>
                            <property name="margin-bottom">20</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkFlowBox">
                            <property name="column-spacing">5</property>
                            <property name="hexpand">True</property>
                            <property name="homogeneous">True</property>
                            <property name="max-children-per-line">4</property>
                            <property name="min-children-per-line">4</property>
                            <property name="row-spacing">5</property>
                            <property name="selection-mode">none</property>
                            <property name="vexpand">True</property>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="a-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="b-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="x-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="y-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="lb-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="rb-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="lp-1-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="lp-2-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="rp-1-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="rp-2-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="dpad-up-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="dpad-down-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="dpad-left-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="dpad-right-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="start-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="select-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="thumbl-info"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="thumbr-info"/>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <style>
                          <class name="info-box"/>
                        </style>
                      </object>
                    </child>
                    <child>
                      <object class="GtkBox">
                        <property name="hexpand">True</property>
                        <property name="orientation">vertical</property>
                        <property name="vexpand">True</property>
                        <child>
                          <object class="GtkLabel">
                            <property name="label">Analog Inputs</property>
                            <property name="margin-bottom">20</property>
                          </object>
                        </child>
                        <child>
                          <object class="GtkFlowBox">
                            <property name="column-spacing">5</property>
                            <property name="hexpand">True</property>
                            <property name="homogeneous">True</property>
                            <property name="max-children-per-line">1</property>
                            <property name="min-children-per-line">4</property>
                            <property name="row-spacing">5</property>
                            <property name="selection-mode">none</property>
                            <property name="vexpand">True</property>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="left-thumbstick"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="right-thumbstick"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="left-trigger"/>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkFlowBoxChild">
                                <child>
                                  <object class="GtkLabel" id="right-trigger"/>
                                </child>
                              </object>
                            </child>
                          </object>
                        </child>
                        <style>
                          <class name="info-box"/>
                        </style>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
